{
  "name": "next-prisma",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-s lint:*",
    "lint:js": "eslint --ext .tsx,.ts src --cache",
    "lint:css": "stylelint './src/**/*.tsx'",
    "fix": "eslint --ext .tsx,.ts src --fix --cache",
    "gql:validate": "ts-graphql-plugin validate",
    "gql:report": "ts-graphql-plugin report",
    "gql:typegen": "ts-graphql-plugin typegen",
    "prisma:generate": "prisma generate"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "@apollo/client": "3.1.3",
    "@prisma/client": "2.5.1",
    "@types/node": "14.6.0",
    "apollo-datasource": "0.7.2",
    "apollo-server-micro": "2.16.1",
    "graphql": "15.3.0",
    "graphql-import": "1.0.2",
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.1.1"
  },
  "devDependencies": {
    "@prisma/cli": "2.5.1",
    "@types/react": "16.9.47",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "import-sort-style-module": "6.0.0",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prettier-plugin-import-sort": "0.0.4",
    "stylelint": "13.6.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "4.1.0",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-graphql-plugin": "1.12.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  }
}
